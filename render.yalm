# render.yaml
services:
  - type: web
    name: career-compass
    env: python
    plan: free
    buildCommand: pip install -r requirements.txt
    startCommand: python main.py
    envVars:
      - key: WEAVIATE_CLOUD_URL
        sync: false
      - key: WEAVIATE_API_KEY  
        sync: false
      - key: OPENAI_API_KEY
        sync: false